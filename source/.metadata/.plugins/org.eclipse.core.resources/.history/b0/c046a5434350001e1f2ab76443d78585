package tut01;

public class PrintEx {

	public static void main(String[] args) {
		// 정수5와 8의 합을 출력하시오.
		//변수선언, 초기화 = 선언과 초기화를 동시
		
//		int num1, num2; //선언
//		num1=5; //초기화
//		num2=8;
		
		//변수이름 첫글자는 소문자
		int num1 = 5;
		int num2 = 8;
		
		//결과값을 변수에 저장 후 출력
		
		int result=num1+ num2;
		
		System.out.println(result);
	    System.out.println(num1+num2);
	}

}
