package tut01;

public class LoopEx {

	public static void main(String[] args) {
		//정수 0 4까지 출력하는 for를 완성하시오
	for(int i = 0; i<5; i++ ) {
		System.out.print(i + " ");
	}
	
	System.out.println("");
	
//0부터 100 미만  출력하는  for 완성
	for(int i2= 0; i2<100; i2++) 
		System.out.print(i2+" ");
	
	//1에서 부터 10 이하까지 출력하는 for
	
	System.out.println(" ");
	for(int i3=1; i3 >= 10; ++i3)
		System.out.print(i3);
	
	}
}
