package tut02;

public class LoopEx {

	public static void main(String[] args) {
		//for(초기값; 조건; 증감값){
		//출력코드
		//}
		
		//0부터 10까지 출력하는 반복문을 완성하시오.
		for(int i=0; i<10; ++i) {
			System.out.print(i+" ");
		}
		//1부터 20까지 출력하는  for
		for(int i2=1;i2<20;++i2) {
			System.out.print(i2+" ");
			
		}
	}
}
