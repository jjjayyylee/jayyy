package tut01;

public class StringEx {

	public static void main(String[] args) {
	// 기본타입  koreait 문자열 저장
		// 객체타입 koreait 문자열 저장
		//비교하는 예제
		 String str="koreait";//stack
		 String str2= new String("koreait111");//heap
		 
		 //비교 A.equals(B) 값
		  if( str.equals(str2 )) {
			  System.out.println("저장위치는 다르지만 값은 동일합니다.");
		  }else
			  System.out.println("값이 다릅니다.");
		 
		 //문자열 비교 == 메모리 주소값
		 
		 //** 문자열을 제외하면 비교는 ==사용하면 됨.
		 

	}

}
