package tut01;

public class AssignmentEx {

	public static void main(String[] args) {
		//증감연산자
		//대입연산자를 줄여서(축약) 사용
		
		int num=5;
		++num;
		System.out.println(num);
		System.out.println(++num);
		System.out.println(num++);
		System.out.println(num);
		num=num+1;
		System.out.println(num);
		
		
		System.out.println(num+=1);
		
		System.out.println(num+=5);
		num *= 10
		
	}

}
