package tut02;

public class OperEx2 {

	public static void main(String[] args) {
		int stu1 = 80;
		int stu2 = 70;
		int avg = 75;
		
		if (stu1>= avg && stu2>=avg) {
			System.out.println("통과");
		}else {
			System.out.println("재평가");
		}
		//AND 이고, 이면서
		//OR 이거나, 또는
		//학생1 점수가 평균점수보다 높고 학생2점수가 평균보다 높으면
		//통과 아니면 재평가
	}

}
