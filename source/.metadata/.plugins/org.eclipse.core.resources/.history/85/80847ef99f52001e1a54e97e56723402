package tut01;

public class LoopEx {

	public static void main(String[] args) {
		//0~10 미만 출력하는 for
		for(int i=0; i<10; ++i) {
			//for를 이용한 출력 01234567
			System.out.print(i+ " ");
			
		}
		
		sysouSystem.out.print(" ");
		
		//0~10 미만 출력하는 while
		
		int i=0;
		while(i<10) {
			System.out.print(i+ " ");
			++i;
		}

	}
}
