package tut01;

public class CastEx {

	public static void main(String[] args) {
		//묵시적 형변환 = 업캐스팅(자동형변환) : 작은 => 큰
		int i = 100;
		float f = i;
		System.out.println(f);
		
		//명시적 형변환= 다운 캐스팅 : 큰 -> 작
		float f2 = 1.23F;
		int num = (int)f2;
		System.out.println(num);
		

	    System.out.println("--------------------------------");
            
	    int cnt =3;
	    int sum = 100;
	    
	    double result =0.0;
	    
	    result = sum/cnt;
	    System.out.println(result);
		

	}

}
